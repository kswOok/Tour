page {
  background-color: #efeff4;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.fixed-block {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.relative-block {
  position: relative;
  width: 100%;
  height: 100%;
}