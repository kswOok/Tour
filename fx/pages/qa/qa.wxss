/* pages/qa/qa.wxss */
page {
  background-color: #fff;
}

.info-title {
  position: relative;
  width: 100%;
  height: 44px;
}

.info-title text {
  height: 24px;
  position: absolute;
  top: 10px;
  left: 10px;
  font-size: 15px;
  color: #333;
  line-height: 24px;
}

.info-value-title {
  padding: 10px 10px 1px;
  font-size: 14px;
  color: #333333;
}

.info-value {
  padding: 1px 10px;
}

.info-value text {
  font-size: 12px;
  color: #999;
}

.info-value text.value-num {
  color: #f19d38;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

 