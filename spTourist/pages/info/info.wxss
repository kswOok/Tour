page {
  background-color: #fff;
}

.info-title {
  position: relative;
  width: 100%;
  height: 44px;
}

.info-title image {
  width: 24px;
  height: 24px;
  position: absolute;
  top: 10px;
  left: 10px;
}

.info-title text {
  height: 24px;
  position: absolute;
  top: 10px;
  left: 40px;
  font-size: 15px;
  color: #333;
  line-height: 24px;
}

.info-value-title {
  padding: 10px 30px 1px;
  font-size: 14px;
  color: #333333;
}

.info-value {
  padding: 1px 30px;
}

.info-value text {
  font-size: 14px;
  color: #999;
}

.info-value text.value-num {
  color: #f19d38;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.school-rank {
  position: relative;
  width: 100%;
  height: 50px;
  padding-left: 30px;
}

.school-rank .rank-num {
  position: absolute;
  top: 10px;
  left: 10px;
  width: 30px;
  height: 30px;
  text-align: center;
  line-height: 30px;
  font-size: 13px;
  color: #ffffff;
  border-radius: 3px;
  background-color: rgb(255, 194, 41);
}

.school-rank .rank-name {
  position: absolute;
  top: 5px;
  left: 50px;
  height: 40px;
  line-height: 40px;
  font-size: 13px;
  color: #333333;
}