page {
  background-color: #ffffff;
}

.form-block {
  position: relative;
  height: 215px;
}

.form-block .form-image {
  width: 100%;
  height: 210px;
}

.form-block .form-mask {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.2);
}

.form-block .form-logo {
  position: absolute;
  top: 50%;
  left: 30px;
  width: 46px;
  height: 46px;
  border: 2px solid #ffffff;
  border-radius: 50%;
}

.form-block .form-name-eng {
  position: absolute;
  top: 30px;
  right: 20px;
  font-size: 17px;
  font-weight: bold;
  color: #ffffff;
}

.form-block .form-name {
  position: absolute;
  top: 90px;
  right: 150px;
  font-size: 20px;
  font-weight: bold;
  color: #ffffff;
}

.form-title-block {
  background-color: #f6f6f6;
  padding: 10px;
  color: #333333;
  position: relative;
}

.form-input-block {
  border-top: 1px solid #e5e5e5;
  border-bottom: 1px solid #e5e5e5;
}

.form-input-block .form-input-item {
  position: relative;
  width: 100%;
  height: 44px;
}

.form-input-block .form-input-item::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 10px;
  right: 10px;
  height: 1px;
  background-color: #e5e5e5;
}

.form-input-block .form-input-item:last-child::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 10px;
  right: 10px;
  height: 1px;
  background-color: transparent;
}

.form-input-block .form-input-item .form-input-name {
  position: absolute;
  top: 10px;
  left: 10px;
  height: 24px;
  line-height: 24px;
  font-size: 15px;
  color: #333333;
}

.form-input-block .form-input-item .form-input-value {
  position: absolute;
  top: 10px;
  left: 120px;
  height: 24px;
  line-height: 24px;
  font-size: 15px;
  color: #999999;
}

.form-input-block .form-input-item input.form-input-value {
  position: absolute;
  top: 10px;
  left: 120px;
  right: 50px;
  height: 24px;
  line-height: 24px;
  font-size: 15px;
  color: #333333;
}

.form-input-block .form-input-item .form-input-arrow {
  position: absolute;
  top: 10px;
  right: 10px;
  width: 24px;
  height: 24px;
}

.checkbox-block {
  width: 20px;
  height: 20px;
  background-color: #ffffff;
  border: 1px solid #e5e5e5;
  border-radius: 2px;
}

.checkbox-block image {
  width: 18px;
  height: 18px;
  margin: 1px;
}

.agree-text {
  font-size: 14px;
  position: absolute;
  left: 40px;
  top: 10px;
  height: 20px;
  line-height: 22px;
  color: #666666;
}

.submit-block {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 54px;
}

.submit-block .submit-btn {
  width: 60%;
  height: 34px;
  margin: 10px auto;
  background-color: #15a2e7;
  color: #ffffff;
  font-size: 15px;
  line-height: 34px;
  text-align: center;
  border-radius: 4px;
}

.loading-mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #ffffff;
}

.loading-img {
  position: absolute;
  width: 80px;
  height: 64px;
  top: 50%;
  left: 50%;
  margin-top: -32px;
  margin-left: -40px;
}

.loading-text {
  position: absolute;
  top: 50%;
  left: 0;
  right: 0;
  margin-top: 42px;
  line-height: 20px;
  color: #15a2e7;
  font-size: 14px;
  text-align: center;
}

.select-item {
  border-radius: 4px;
  font-size: 15px;
  display: inline-block;
  margin: 5px;
  padding: 8px 14px;
  background-color: #e5e5e5;
  color: #999999;
}

.select-item.active {
  background-color:#15a2e7;
  color:#ffffff;
}
::-webkit-scrollbar {
 width: 0;
 height: 0;
 color: transparent;
}